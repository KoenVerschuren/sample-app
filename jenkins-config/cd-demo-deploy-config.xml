<?xml version="1.0" encoding="UTF-8"?>
<project>
  <actions/>
  <description/>
  <keepDependencies>false</keepDependencies>
  <properties>
    <hudson.queueSorter.PrioritySorterJobProperty>
      <priority>100</priority>
    </hudson.queueSorter.PrioritySorterJobProperty>
    <hudson.plugins.throttleconcurrents.ThrottleJobProperty>
      <maxConcurrentPerNode>0</maxConcurrentPerNode>
      <maxConcurrentTotal>0</maxConcurrentTotal>
      <throttleEnabled>false</throttleEnabled>
      <throttleOption>project</throttleOption>
    </hudson.plugins.throttleconcurrents.ThrottleJobProperty>
    <de.pellepelster.jenkins.walldisplay.WallDisplayJobProperty/>
  </properties>
  <scm class="hudson.scm.SubversionSCM">
    <locations>
      <hudson.scm.SubversionSCM_-ModuleLocation>
        <remote>http://localhost/svn/sample-app</remote>
        <local>.</local>
      </hudson.scm.SubversionSCM_-ModuleLocation>
    </locations>
    <excludedRegions/>
    <includedRegions/>
    <excludedUsers/>
    <excludedRevprop/>
    <excludedCommitMessages/>
    <workspaceUpdater class="hudson.scm.subversion.UpdateUpdater"/>
  </scm>
  <canRoam>true</canRoam>
  <disabled>false</disabled>
  <blockBuildWhenDownstreamBuilding>false</blockBuildWhenDownstreamBuilding>
  <blockBuildWhenUpstreamBuilding>false</blockBuildWhenUpstreamBuilding>
  <triggers class="vector"/>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Maven>
      <targets>clean verify -Pjenkins</targets>
      <mavenName>Default maven</mavenName>
      <pom>cd-demo-dar/pom.xml</pom>
      <usePrivateRepository>false</usePrivateRepository>
    </hudson.tasks.Maven>
  </builders>
  <publishers>
    <com.xebialabs.deployit.ci.DeployitNotifier>
      <credential>Admin</credential>
      <application>Applications/cd-demo</application>
      <version>1.0.0.0</version>
      <packageOptions>
        <deployables>
          <com.xebialabs.deployit.ci.ArtifactView>
            <type>jee.War</type>
            <name>cd-service-war</name>
            <tags/>
            <location>cd-demo-dar/target/dependency/cd-service-war.war</location>
          </com.xebialabs.deployit.ci.ArtifactView>
          <com.xebialabs.deployit.ci.ArtifactView>
            <type>sql.SqlScripts</type>
            <name>SQL scripts</name>
            <tags/>
            <location>cd-demo-dar/target/dependency/cd-demo-sql-resources.zip</location>
          </com.xebialabs.deployit.ci.ArtifactView>
          <com.xebialabs.deployit.ci.ArtifactView>
            <type>www.WebContent</type>
            <name>HTML files</name>
            <tags/>
            <location>cd-demo-dar/target/dependency/cd-demo-html-resources.zip</location>
          </com.xebialabs.deployit.ci.ArtifactView>
        </deployables>
      </packageOptions>
      <importOptions>
        <mode class="com.xebialabs.deployit.ci.GeneratedLocation"/>
      </importOptions>
      <deploymentOptions>
        <skipMode>false</skipMode>
        <testMode>false</testMode>
        <deletePreviouslyDeployedArtifact>false</deletePreviouslyDeployedArtifact>
        <failIfNoStepsAreGenerated>false</failIfNoStepsAreGenerated>
        <explicitMappings>false</explicitMappings>
        <generateDeployedOnUpgrade>true</generateDeployedOnUpgrade>
        <cancelTaskOnError>true</cancelTaskOnError>
        <rollbackOnError>false</rollbackOnError>
        <orchestrator/>
        <environment>Environments/CD-DEMO</environment>
        <verbose>false</verbose>
        <versionKind>Packaged</versionKind>
      </deploymentOptions>
    </com.xebialabs.deployit.ci.DeployitNotifier>
  </publishers>
  <buildWrappers/>
</project>